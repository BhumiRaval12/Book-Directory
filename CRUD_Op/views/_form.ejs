     <form id="open-form" action="/insert" method="POST">
         
              <div class="form-group">
                   <label for="name">Course-Name</label>
                   <input type="text" class="form-control" name="name" id="name" placeholder="Course-Name">
              </div>
              <div class="form-group">
                   <label for="duration">Course-Duration</label>
                   <input type="text" class="form-control" name="duration" id="duration" placeholder="Course-Duration">
              </div>
              <div class="form-group">
                   <label for="fees">Course-Fees</label>
                   <input type="text" class="form-control" name="fees" id="fees" placeholder="Course-Fees">
              </div>
              
              <div>
                   <button type="submit" class="adbtn">Submit</button>
                

              </div>
             

         
     </form>
